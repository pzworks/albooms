{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react-native",
    "lib": [
      "ES6",
      "dom",
      "esnext"
    ],
    "moduleResolution": "node",
    "isolatedModules": true,
    "strict": true,
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strictPropertyInitialization": false,
    "noUnusedLocals": false
  },
  "extends": "expo/tsconfig.base"
}
